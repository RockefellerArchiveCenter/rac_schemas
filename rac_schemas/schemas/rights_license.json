{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "rights_license.json",
	"type": "object",
	"title": "RAC Rights License Schema",
	"properties": {
		"license_identifier": {
			"$id": "#/properties/license_identifier",
			"type": "string",
			"title": "License Documentation Identifier",
			"minLength": 1
		},
		"license_terms": {
			"$id": "#/properties/license_terms",
			"type": "string",
			"title": "License Terms"
		},
		"determination_date": {
			"$id": "#/properties/determination_date",
			"type": ["string", "null"],
			"title": "License Determination Date",
			"examples": [
				"2019-08-23"
			]
		}
	}
}
