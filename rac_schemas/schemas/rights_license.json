{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "rights_license.json",
	"type": "object",
	"title": "RAC Rights License Schema",
	"properties": {
		"license_identifier": {
			"$id": "#/license_identifier",
			"type": "string",
			"title": "License Documentation Identifier",
			"minLength": 1,
			"maxLength": 250
		},
		"license_terms": {
			"$id": "#/license_terms",
			"type": "string",
			"title": "License Terms",
			"maxLength": 2500,
		},
		"determination_date": {
			"$id": "#/determination_date",
			"type": ["string", "null"],
			"title": "License Determination Date",
			"examples": [
				"2019-08-23"
			]
		},
		"start_date": {
			"$id": "#/start_date",
			"type": ["string", "null"],
			"title": "License Begin Date",
			"examples": [
				"2019-08-23"
			]
		},
		"end_date": {
			"$id": "#/end_date",
			"type": ["string", "null"],
			"title": "License End Date",
			"examples": [
				"2019-08-23",
				"open"
			]
		},
		"license_note": {
			"$id": "#/license_note",
			"type": "array",
			"title": "License Note",
			"items": {
				"type": "string",
				"maxLength": 2500
			}
		}
	}
}
