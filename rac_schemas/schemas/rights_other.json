{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "rights_other.json",
	"type": "object",
	"title": "RAC Rights Other Basis Schema",
	"required": [
		"other_basis"
	],
	"properties": {
		"other_identifier": {
			"$id": "#/properties/other_identifier",
			"type": "string",
			"title": "Other Documentation Identifier",
			"minLength": 1
		},
		"other_basis": {
			"$id": "#/properties/other_basis",
			"type": "string",
			"title": "Other Rights Basis",
			"enum": [
				"donor",
				"policy"
			]
		}
	}
}
